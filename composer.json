{
    "name": "cinghie/yii2-paypal",
    "description": "Yii2 Paypal to manage Paypal Payments on your Yii 2 project",
    "keywords": [
	    "yii2 paypal", 
	    "yii2 payments", 
	    "yii2 paypal payments", 
	    "paypal", 
	    "payments"
    ],
    "homepage": "https://github.com/cinghie/yii2-paypal",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Gogodigital.it",
            "email": "info@gogodigital.it",
            "homepage": "http://www.gogodigital.it/"
        }
    ],
    "require": {
	"php": ">=5.5.0",
	"ext-curl": "*",
	"ext-json": "*",
	"yiisoft/yii2": "~2.0.14",
        "braintree/braintree_php": "^6.11.2",
        "hyperwallet/sdk": "^2.2.4",
        "paypal/merchant-sdk-php": "@dev",
        "paypal/rest-api-sdk-php": "dev-main",
        "cinghie/yii2-traits": "@dev"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "cinghie\\paypal\\": ""
        }
    }
}
